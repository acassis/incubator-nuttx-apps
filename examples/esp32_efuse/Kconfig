#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_ESP32_EFUSE
	tristate "ESP32 EFUSE Example"
	default n
	---help---
		ESP32 EFUSE Example

if EXAMPLES_ESP32_EFUSE

config EXAMPLES_ESP32_EFUSE_PROGNAME
	string "Program name"
	default "esp32efuse"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config EXAMPLES_ESP32_EFUSE_PRIORITY
	int "ESP32 EFUSE task priority"
	default 100

config EXAMPLES_ESP32_EFUSE_STACKSIZE
	int "ESP32 EFUSE stack size"
	default DEFAULT_TASK_STACKSIZE

endif
